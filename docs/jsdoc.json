{
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-tsimport-plugin/index.js"
  ],
  "source": {
    "include": ["packages/browser/src/Component/Widget/SlideShow"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "sourceType": "module",
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "search": true
  },
  "opts": {
    "destination": "./jsdoc",
    "recurse": true,
    "readme": "./readme.md",
    "template": "node_modules/better-docs"
  },
  "tags": {
    "allowUnknownTags": ["optional"],
    "dictionaries": ["jsdoc", "closure"]
  }
}
