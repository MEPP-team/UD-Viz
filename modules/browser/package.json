{
  "name": "ud-viz",
  "version": "2.39.3",
  "description": "A framework extension of itowns.",
  "main": "src",
  "scripts": {
    "travis": "npm run eslint && npm run build",
    "eslint-quiet": "eslint --quiet \"src/**/*.js\" \"examples/**/*.js\"",
    "eslint": "eslint \"src/**/*.js\" \"examples/**/*.js\"",
    "eslint-fix": "eslint \"src/**/*.js\" \"examples/**/*.js\" --fix",
    "build": "cross-env NODE_ENV=production webpack --config=./config/webpack.config.js",
    "build-debug": "cross-env NODE_ENV=development webpack --config=./config/webpack.config.js",
    "debug": "nodemon --trace-warnings --verbose --watch src --delay 2500ms ./bin/debug.js -e js,css,html",
    "docs": "jsdoc -c ./docs/jsdoc.json --verbose"
  },
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VCityTeam/UD-Viz.git"
  },
  "license": "(LGPL-2.1)",
  "bugs": {
    "url": "https://github.com/VCityTeam/UD-Viz/issues"
  },
  "homepage": "https://github.com/VCityTeam/UD-Viz/",
  "dependencies": {
    "@deck.gl/core": "^8.5.0",
    "@deck.gl/layers": "^8.5.0",
    "buffer": "^6.0.3",
    "d3": "^7.0.1",
    "detect-collisions": "^2.5.2",
    "fetch-sparql-endpoint": "^2.3.1",
    "howler": "^2.2.3",
    "itowns": "2.38.0",
    "jquery": "^3.6.0",
    "proj4": "^2.7.5",
    "socket.io-client": "^4.0.0",
    "three": "0.137.5",
    "vis-data": "^7.1.2",
    "vis-network": "^9.0.3"
  },
  "dependencyComments": {
    "d3": "required by the SPARQL Module"
  },
  "devDependencies": {
    "child-process-promise": "^2.2.1",
    "clean-jsdoc-theme": "^3.3.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jsdoc": "^3.6.7",
    "nodemon": "^2.0.12",
    "prettier": "2.6.2",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  }
}
