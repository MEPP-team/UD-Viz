  "bugs": {
    "url": "https://github.com/VCityTeam/UD-Viz/issues"
  },
  "homepage": "https://github.com/VCityTeam/UD-Viz/packages/shared",
  "dependencies": {
    "detect-collisions": "2.5.2"
  },
  "peerDependencies": {
    "three": "0.154.0"
  }
}
