{
  "name": "@ud-viz/shared",
  "version": "3.2.0",
  "description": "UD-Viz shared Browser+Node framework",
  "main": "src",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config=./webpackConfig/webpack.config.js",
    "build-debug": "cross-env NODE_ENV=development webpack --config=./webpackConfig/webpack.config.js",
    "test": "node --trace-warnings ./bin/test.js",
    "debug": "nodemon --trace-warnings --verbose --watch src --watch ./bin/Test/**/*.js --delay 2500ms ./bin/debug.js -e js"
  },
  "files": [
    "src"
  ],
